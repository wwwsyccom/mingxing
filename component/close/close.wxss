@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}
.icon {
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  align-items: center;
}
.icon::before {
  display: block;
  font-family: "iconfont" !important;
  font-size: 24rpx;
  margin-right: 12rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.icon-after::after {
  font-family: "iconfont" !important;
  font-size: 24rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.img-wrap {
  overflow: hidden;
}
.img-wrap image {
  width: 100%;
  height: 100%;
}
.button {
  clear: both;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: white;
  border-radius: 12rpx;
  background-color: #3323B2;
  margin: 0 auto;
  padding: 0;
}
.close {
  width: 48rpx;
  height: 48rpx;
  border: 1px solid #ececec;
  border-radius: 100rpx;
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  align-items: center;
  background-color: white;
  position: absolute;
  top: -10rpx;
  right: -10rpx;
}
.close:before {
  content: '\e63d';
  margin: 0;
  font-size: 24rpx;
  color: #979797;
}
