@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}
.icon {
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  align-items: center;
}
.icon::before {
  display: block;
  font-family: "iconfont" !important;
  font-size: 24rpx;
  margin-right: 12rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.star {
  padding: 40rpx;
  background-color: white;
  height: 396rpx;
  display: flex;
  display: -webkit-flex;
}
.star .photo,
.star .detal {
  height: 100%;
}
.star .photo {
  width: 288rpx;
  margin-right: 20rpx;
  overflow: hidden;
}
.star .photo image {
  width: 100%;
  height: 100%;
}
.star .detail {
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.star .detail .top-item {
  margin-bottom: 20rpx;
  font-size: 28rpx;
}
.star .detail .name-wrap {
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  justify-content: space-between;
}
.star .detail .name-wrap .name {
  font-size: 36rpx;
  color: #444444;
}
.star .detail .name-wrap .dailed {
  font-size: 28rpx;
}
.star .detail .name-wrap .dailed:before {
  content: '\e603';
  color: #3323B2;
  position: relative;
  top: 4rpx;
}
.star .detail .name-wrap .dailed .desc {
  color: #444444;
  opacity: 0.82;
}
.star .detail .key {
  color: #444444;
  opacity: 0.55;
  margin-right: 10rpx;
}
.star .detail .value {
  color: #444444;
  opacity: 0.82;
}
.star .detail .ident {
  color: #444444;
  opacity: 0.82;
  font-size: 28rpx;
}
.star .detail .dail {
  margin-bottom: 0px;
  width: 248rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #3323B2;
  color: white;
  text-align: center;
  border-radius: 200rpx;
  font-size: 28rpx;
}
