@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}
.icon {
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  align-items: center;
}
.icon::before {
  display: block;
  font-family: "iconfont" !important;
  font-size: 24rpx;
  margin-right: 12rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.icon-after::after {
  font-family: "iconfont" !important;
  font-size: 24rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.img-wrap {
  overflow: hidden;
}
.img-wrap image {
  width: 100%;
  height: 100%;
}
page {
  padding-top: 20rpx;
}
.list {
  padding: 20rpx 40rpx;
  background-color: white;
  border-bottom: 1px solid #ececec;
  display: flex;
  display: -webkit-flex;
}
.list .img-wrap {
  height: 120rpx;
  width: 120rpx;
  margin-right: 16rpx;
  border-radius: 200rpx;
}
.list .desc {
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 6rpx 0rpx;
}
.list .desc .top {
  display: flex;
  display: -webkit-flex;
  align-items: center;
  color: #989898;
  font-size: 28rpx;
}
.list .desc .top .name {
  color: #444444;
  font-size: 36rpx;
  margin-right: 20rpx;
}
.list .desc .below {
  font-size: 28rpx;
  color: #666666;
}
.list .desc .below .num {
  margin-left: 10rpx;
  color: #989898;
}
