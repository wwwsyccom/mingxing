@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}
.icon {
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  align-items: center;
}
.icon::before {
  display: block;
  font-family: "iconfont" !important;
  font-size: 24rpx;
  margin-right: 12rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.icon-after::after {
  font-family: "iconfont" !important;
  font-size: 24rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.img-wrap {
  overflow: hidden;
}
.img-wrap image {
  width: 100%;
  height: 100%;
}
page {
  padding-top: 20rpx;
  height: 100vh;
  display: flex;
  display: -webkit-flex;
  overflow: hidden;
}
.wrap {
  width: 100%;
  background-color: white;
  padding: 86rpx 56rpx 50rpx;
  overflow: auto;
}
.wrap .logo-wrap {
  text-align: center;
  margin-bottom: 24rpx;
}
.wrap .logo-wrap image {
  width: 82rpx;
  height: 68rpx;
}
.wrap .title {
  margin-bottom: 20rpx;
  font-size: 28rpx;
  color: #666666;
  line-height: 48rpx;
}
.wrap .content {
  margin-bottom: 40rpx;
  color: #989898;
  font-size: 28rpx;
  line-height: 48rpx;
  display: block;
}
